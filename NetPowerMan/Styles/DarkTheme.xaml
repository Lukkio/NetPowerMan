<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="LabelForeground" Color="LightGray"/>
    <SolidColorBrush x:Key="BorderColor" Color="LightGray"/>
    <SolidColorBrush x:Key="ButtonSeparatorColor" Color="#96707070"/>
    <SolidColorBrush x:Key="WindowBackgrounColor" Color="#452e2e2e"/>
    <SolidColorBrush x:Key="WindowBackgrounColor2" Color="{Binding BackgroundWindowsColor}"/>
    <CornerRadius x:Key="WindowsCornerRadius" TopLeft="0" TopRight="0" BottomRight="0" BottomLeft="0"/>
    <CornerRadius x:Key="ButtonCornerRadius" TopLeft="10" TopRight="10" BottomRight="10" BottomLeft="10"/>
    <BitmapImage x:Key="ShutdownButtonImage" UriSource="/Resources/shutdown_w.png" />
    <BitmapImage x:Key="RebootButtonImage" UriSource="/Resources/reboot_w.png" />
    <BitmapImage x:Key="RebootSmallButtonImage" UriSource="/Resources/reboot24_w.png" />
    <BitmapImage x:Key="PoweroffSmallButtonImage" UriSource="/Resources/poweroff20_w.png" />
    <BitmapImage x:Key="SettingSmallButtonImage" UriSource="/Resources/setting_w.png" />
    <BitmapImage x:Key="CloseSmallButtonImage" UriSource="/Resources/cancelclose_w.png" />
    <BitmapImage x:Key="Linked" UriSource="/Resources/linked_w.png" />
    <BitmapImage x:Key="Broken" UriSource="/Resources/broken_w.png" />
</ResourceDictionary>
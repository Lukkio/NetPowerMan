             xmlns:app="clr-namespace:NetPowerMan.Views"
             xmlns:local="clr-namespace:NetPowerMan">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/DarkTheme.xaml" />
                <ResourceDictionary Source="/Styles/ContextMenu.xaml" />
                <ResourceDictionary Source="/Styles/ToolTip.xaml" />
                <ResourceDictionary Source="/Styles/ExpanderCustom.xaml" />
                <ResourceDictionary Source="/Styles/CheckBoxCustom.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
